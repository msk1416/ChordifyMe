{
    "manifest_version": 2,
    "name": "ChordifyMe",
    "description": "Chordify description text",
    "version": "0.0.1",
    "minimum_chrome_version": "38",
    "author": "Sergi Mascar√≥",
    "icons" : {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "64": "icons/icon_64.png",
        "128": "icons/icon_128.png"
    },
    "page_action":{
        "default_title" : "Only available on youtube videos"
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "*://*/*",
        "declarativeContent"
    ],
    "background": {
        "scripts" : ["background.js"]
    }
}